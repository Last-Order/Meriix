        "vite-plugin-electron-renderer": "^0.14.1",
        "vue-tsc": "^1.2.0"
    },
    "dependencies": {
        "@mdi/font": "^7.2.96",
        "axios": "^1.4.0",
        "electron-log": "^4.4.8",
        "markdown-it-vue": "^1.1.7",
        "tree-kill": "^1.2.2",
        "vue": "^3.2.47",
        "vue-markdown-render": "^2.0.1",
        "vuetify": "^3.2.3",
        "vuex": "^4.1.0"
